function valorComDesconto(valor) {
    let desconto = 0;

    if (valor > 20) {
        desconto = valor * 20 / 100; // Calcula 20% de desconto
        return "Valor com desconto: " + (valor - desconto).toFixed(2);
    } else {
        return "Valor sem desconto: " + valor.toFixed(2);
    }
}

console.log(valorComDesconto(30)); // Valor com desconto: 24.00
console.log(valorComDesconto(5));  // Valor sem desconto: 5.00
